{% extends 'Base.html.twig' %}

{% block AjouterEtablissement %}

    <section class="page_header">
        <div class="container">
            <div class="display-table">
                <div class="table-cell">
                    <div class="page_content align-center">
                        <h2 class="text-white">Voulez Vous Ajouter Un Etablissement ?</h2>
                        <p>Contactez-Nous Et Devenez L'Administrateur De La Page De Votre Etablissement</p>
                    </div>
                </div>
            </div>
        </div>
    </section><!-- /.page_header -->

